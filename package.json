{
  "name": "cf-server",
  "version": "0.0.1",
  "private": true,
  "author": "Patrick Recher <p@tte.io>",
  "dependencies": {
    "@project-r/auth-server": "*",
    "babel-cli": "6.14.0",
    "babel-core": "6.14.0",
    "babel-preset-es2015": "6.14.0",
    "babel-preset-stage-0": "6.5.0",
    "body-parser": "^1.15.2",
    "cookie-parser": "^1.4.3",
    "dotenv": "^4.0.0",
    "express": "^4.14.0",
    "express-session": "^1.14.2",
    "graphql": "^0.8.2",
    "graphql-server-express": "^0.5.0",
    "graphql-tag": "^1.2.3",
    "graphql-tools": "^0.9.0",
    "pg": "^6.1.2",
    "sequelize": "^3.30.0"
  },
  "scripts": {
    "start": "nodemon index.js --exec babel-node",
    "build": "mkdir -p ./dist && npm run build-client && npm run build-server",
    "build-client": "rm -rf ./dist/client_build && cd ../cf_admin && npm run build && mv ./build ../cf_server/dist/client_build",
    "build-server": "NODE_ENV=production babel index.js -d dist --presets es2015,stage-2",
    "start-production": "NODE_ENV=production node dist/index.js",
    "dev": "echo \"This command has been deprecated. Use 'npm start'\" && exit 1"
  },
  "devDependencies": {
    "babel-preset-latest": "^6.22.0",
    "concurrently": "^3.1.0",
    "http-proxy": "^1.16.2",
    "nodemon": "^1.11.0"
  }
}
