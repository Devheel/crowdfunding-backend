{
  "name": "cf-server",
  "version": "0.0.1",
  "engines": {
    "node": "7.7.4"
  },
  "private": true,
  "author": "Patrick Recher <p@tte.io>",
  "dependencies": {
    "aws-sign": "^0.1.2",
    "body-parser": "^1.17.1",
    "connect-pg-simple": "^3.1.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.3",
    "d3-array": "^1.2.0",
    "d3-format": "^1.2.0",
    "d3-time-format": "^2.0.5",
    "db-migrate": "^0.10.0-beta.20",
    "db-migrate-pg": "^0.1.11",
    "dotenv": "^4.0.0",
    "express": "^4.15.2",
    "express-basic-auth": "^1.0.1",
    "express-session": "^1.15.2",
    "geoip-database": "^1.0.2",
    "graphql": "^0.9.3",
    "graphql-server-express": "^0.7.2",
    "graphql-tag": "^2.0.0",
    "graphql-tools": "^0.11.0",
    "gsheets": "^2.0.0",
    "isomorphic-unfetch": "^1.0.0",
    "knex": "^0.12.9",
    "kraut": "^0.1.1",
    "maxmind": "^2.2.0",
    "mz": "^2.6.0",
    "optics-agent": "^1.1.2",
    "passport": "^0.3.2",
    "pg": "^6.1.5",
    "pogi": "^2.2.1",
    "querystring": "^0.2.0",
    "random-noun-generator-german": "^0.1.3",
    "sharp": "^0.17.3",
    "slugify": "^1.1.0",
    "stripe": "^4.18.0",
    "uuid": "^3.0.1",
    "winston": "^2.3.1"
  },
  "scripts": {
    "dev": "NODE_ENV=development nodemon -w graphql -w src -w lib -w index.js index.js",
    "debug": "NODE_ENV=development node-debug index.js",
    "start": "node index.js",
    "migrate:up": "db-migrate up",
    "migrate:down": "db-migrate down",
    "db:reset": "db-migrate reset && db-migrate up && knex seed:run",
    "seed": "knex seed:run",
    "test": "jest --coverage",
    "test:watch": "jest --watch --coverage",
    "translations": "gsheets --key=1OHoiENwJH-tqf9yXfXHOevAn17daO_CvB21Rym-oiqk --title=live --pretty --out lib/translations.json",
    "import": "cat script/local/members.csv | node script/importMembers.js && node script/importTestimonials.js && node script/generateSMImages.js",
    "sync:testimonials": "s3cmd sync --verbose --acl-public -c ./.s3cfg s3://republik-staging/testimonials/ script/local/testimonials/",
    "sync:sm": "s3cmd sync --verbose --acl-public -c ./.s3cfg s3://republik-staging/testimonials/sm/ script/local/testimonials/sm/"
  },
  "devDependencies": {
    "concurrently": "^3.4.0",
    "d3-dsv": "^1.0.5",
    "gsheets": "^2.0.0",
    "http-proxy": "^1.16.2",
    "jest": "^19.0.2",
    "jest-cli": "^19.0.2",
    "node-inspector": "^1.1.0",
    "nodemon": "^1.11.0",
    "rw": "^1.3.3"
  },
  "jest": {
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "coverageReporters": [ "lcov", "html" ],
    "coverageDirectory": "./coverage/",
    "collectCoverage": true
  }
}
